function session_application(checknum, pid, args) {
	try {
		eyeos.cleanSession();
	} catch (e) {
		eyeos.consoleWarn(e);
	}
	
	if (args) {
		// User data
		if (args[0]) {
			eyeos.setCurrentUserData(args[0]);
		}
		// Init apps
		if (args[1]) {
			for(var i = 0; i < args[1].length; i++) {
				eyeos.execute(args[1][i], checknum);
			}
		}
	}
}

